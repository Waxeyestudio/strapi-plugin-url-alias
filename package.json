{
  "name": "@strapi-community/strapi-plugin-url-alias",
  "version": "1.0.0-alpha.10",
  "description": "Unique, autogenerated URL paths.",
  "strapi": {
    "displayName": "URL alias",
    "name": "url-alias",
    "icon": "path",
    "description": "Unique, autogenerated URL paths.",
    "required": false,
    "kind": "plugin"
  },
  "scripts": {
    "eslint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "eslint --fix './**/*.{js,jsx,ts,tsx}'",
    "test:unit": "jest --verbose",
    "plugin:install": "yarn install && rm -rf node_modules/@strapi/helper-plugin",
    "develop:server": "tsc -p tsconfig.server.json -w",
    "develop:admin": "tsc -p tsconfig.json -w",
    "develop": "run-p develop:*",
    "build:server": "tsc -p tsconfig.server.json",
    "build:admin": "tsc -p tsconfig.json",
    "build:copy": "copyfiles package.json dist/",
    "build": "rimraf dist && yarn build:server && yarn build:admin && yarn build:copy"
  },
  "author": {
    "name": "Boaz Poolman",
    "email": "info@boazpoolman.nl",
    "url": "https://github.com/boazpoolman"
  },
  "maintainers": [
    {
      "name": "Boaz Poolman",
      "email": "info@boazpoolman.nl",
      "url": "https://github.com/boazpoolman"
    }
  ],
  "files": [
    "LICENSE.md",
    "dist",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "peerDependencies": {
    "@strapi/strapi": "^4.14.0"
  },
  "devDependencies": {
    "@strapi/design-system": "^1.8.0",
    "@strapi/helper-plugin": "^4.14.3",
    "@strapi/icons": "^1.8.0",
    "@strapi/strapi": "^4.14.3",
    "@strapi/utils": "^4.14.3",
    "@types/lodash": "^4.14.195",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-eslint": "9.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-styled-components": "^7.1.1",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-intl": "^5.20.12",
    "react-query": "^3.39.3",
    "rimraf": "^5.0.1",
    "styled-components": "^5.2.3",
    "typescript": "^5.2.2",
    "yup": "^1.2.0"
  },
  "bugs": {
    "url": "https://github.com/strapi-community/strapi-plugin-url-alias/issues"
  },
